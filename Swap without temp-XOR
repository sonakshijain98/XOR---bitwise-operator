//XOR
#include <iostream>
using namespace std;
int main()
{
     int x,y;
     cin>>x>>y;
     x=x^y; //x=7 -> 5^2
     y=x^y; //y=5 -> 7^2
     x=x^y; //x=2 -> 7^5
     cout<<"x: "<<x<<" y: "<<y;
    return 0;
}


//Normal approach
#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int x = 10, y = 5;
 
    // Code to swap 'x' and 'y'
    x = x + y; // x now becomes 15
    y = x - y; // y becomes 10
    x = x - y; // x becomes 5
    cout << "After Swapping: x =" << x << ", y=" << y;
}
 
